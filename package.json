{
  "name": "goal-measurement",
  "version": "2.0.0",
  "description": "Backend for the goal measurement project",
  "main": "server.js",
  "scripts": {
    "start": "node ./index.js",
    "server": "nodemon index.js --ignore client/",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristianrodri/goal-measurement.git"
  },
  "keywords": [
    "backend",
    "node",
    "express",
    "mongodb"
  ],
  "author": "Cristian Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cristianrodri/goal-measurement/issues"
  },
  "homepage": "https://github.com/cristianrodri/goal-measurement#readme",
  "devDependencies": {
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "express-jwt": "^6.0.0",
    "jimp": "^0.10.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.27",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "typescript": "^4.2.3",
    "validator": "^13.1.1"
  }
}
